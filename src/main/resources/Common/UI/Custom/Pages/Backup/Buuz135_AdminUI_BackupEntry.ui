$C = "../../Common.ui";
Group {
  LayoutMode: Top;
  Background: $C.@InputBoxBackground;
  Padding: (Top: 5, Bottom: 3);
  Anchor: ( Bottom: 5);
  Group {
    LayoutMode: Left;
    
    FlexWeight: 1;
    

    Label #BackupName {
        Style: (
        FontSize: 16,
        RenderBold: true,
        TextColor: #ffffff
        );
        Anchor: (Left: 10, Vertical: 0);
        Padding: (Right: 2);
    }

    Label #BackupSize {
        Style: (
        FontSize: 12,
        RenderBold: false,
        TextColor: #ffffff(0.3)
        );
        Anchor: (Left: 1, Top: 4);
        Padding: (Right: 5);
    }

    Label {
        FlexWeight: 1;
    }

    $C.@CancelTextButton #RemoveMemberButton {
        Anchor: (Right: 10, Vertical: 0, Height: 20);
        Padding: (Left: 8, Right: 8, Top: 4, Bottom: 4);
        Text: "Remove";
        Overscroll: false;
    }
  }

  Group {
    LayoutMode: Left;
    Anchor: (Left: 10, Vertical: 0);
  
    Label #TimeLeft {
      Style: (
          FontSize: 14,
          RenderBold: false,
          TextColor: #ffffff
      );
    }

    Label {
      Anchor: ();
      Style: (
        FontSize: 14,
        TextColor: #93844c,
          RenderBold: true
      );
      Text: " old";
    }

    
  }

}